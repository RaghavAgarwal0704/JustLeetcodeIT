class Solution {
    public boolean isPalindrome(int x) {
        if(x<0)return false;
        else
        {
            int n=0;int r=x;
            while(x!=0)
            {
                n=n*10+x%10;
                x=x/10;
            }
            x=r;
            if(x==n)return true;
            else return false;
        }
    }
}
